#ctype CString "char*"
#ctype CVoidPtr "void*"
#ctype CSizeT "size_t"
#ctype Int32 "int"
#ctype Int64 "long long"
#ctype Float32 "float"
#ctype Float64 "double"
#ctype Bool "unsigned char"
#ctype Void "void"

#no_remap
#native_function
#native_header "stdlib.h"
func alcl_gc_alloc(size: CSizeT): CVoidPtr {
    return malloc(size);
}
